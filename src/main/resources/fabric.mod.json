{
  "schemaVersion": 1,
  "id": "lootinject",
  "version": "${version}",
  "name": "LootInject",
  "description": "A simple mod that allows injecting a loot table into an existing table",
  "authors": ["Virusnest"],
  "contact": {},
  "license": "MIT",
  "icon": "assets/lootinject/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "me.virusnest.lootinject.Lootinject"
    ]
  },
  "mixins": [
    "lootinject.mixins.json",
    {
      "config": "lootinject.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
